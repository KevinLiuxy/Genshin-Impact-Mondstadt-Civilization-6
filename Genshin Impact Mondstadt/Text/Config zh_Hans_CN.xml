		<Row Language="zh_Hans_CN" Tag="LOC_CIVILIZATION_GENSHIN_MONDSTADT_DESCRIPTION"
			Text="蒙德"/>
		<Row Language="zh_Hans_CN" Tag="LOC_CIVILIZATION_GENSHIN_MONDSTADT_ADJECTIVE"
